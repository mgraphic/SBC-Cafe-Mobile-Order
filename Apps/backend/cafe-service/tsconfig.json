{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ESNext",
        "rootDir": "./src",
        "outDir": "./dist",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "moduleResolution": "bundler",
        "sourceMap": true,
        "resolveJsonModule": true
    }
}
